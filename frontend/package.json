{
	"name": "3d-annotator",
	"version": "0.1.4",
	"private": true,
	"dependencies": {
		"@t3-oss/env-core": "^0.6.0",
		"axios": "^0.27.2",
		"daisyui": "^2.27.0",
		"fflate": "^0.8.2",
		"lucide-react": "^0.446.0",
		"neverthrow": "^8.0.0",
		"react": "^18.3.1",
		"react-cookie": "^7.2.0",
		"react-dom": "^18.3.1",
		"react-loader-spinner": "^5.3.4",
		"react-router-dom": "^6.26.2",
		"react-toastify": "^9.0.8",
		"tailwindcss": "^3.1.8",
		"three": "^0.169.0",
		"three-mesh-bvh": "^0.8.1",
		"typesafe-i18n": "^5.26.2",
		"zod": "^3.23.8",
		"zustand": "^4.5.5"
	},
	"scripts": {
		"start": "vite",
		"build": "pnpm ts && vite build",
		"preview": "vite preview",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"test:coverage": "pnpm test --coverage",
		"checks:all": "pnpm checks:required && pnpm checks:optional",
		"checks:required": "pnpm ts && pnpm lint && pnpm format:check",
		"checks:optional": "pnpm lint:warnings && pnpm spellcheck",
		"ts": "pnpm i18n && tsc",
		"ts:watch": "pnpm i18n && tsc --watch",
		"lint": "pnpm i18n && eslint \"src/**/*.{js,jsx,ts,tsx}\"",
		"lint:warnings": "pnpm lint --max-warnings=0",
		"lint:fix": "pnpm lint --fix",
		"prettier": "prettier \"**/*.{js,jsx,ts,tsx,json,html,css}\"",
		"format": "pnpm prettier --write",
		"format:check": "pnpm prettier --check",
		"spellcheck": "cspell .",
		"prepare": "husky install && pnpm i18n",
		"pre-commit": "pretty-quick --staged",
		"i18n": "typesafe-i18n --no-watch",
		"i18n:watch": "typesafe-i18n"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@cspell/dict-css": "^4.0.13",
		"@cspell/dict-de-de": "^4.0.0",
		"@cspell/dict-en-common-misspellings": "^2.0.4",
		"@cspell/dict-filetypes": "^3.0.4",
		"@cspell/dict-html": "^4.0.6",
		"@cspell/dict-markdown": "^2.0.2",
		"@cspell/dict-npm": "^5.1.5",
		"@cspell/dict-software-terms": "^4.1.4",
		"@cspell/dict-typescript": "^3.1.6",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^22.7.4",
		"@types/react": "^18.3.10",
		"@types/react-dom": "^18.3.0",
		"@types/testing-library__jest-dom": "^5.14.8",
		"@types/three": "^0.169.0",
		"@types/wicg-file-system-access": "^2023.10.5",
		"@typescript-eslint/eslint-plugin": "^6.1.0",
		"@typescript-eslint/parser": "^6.1.0",
		"@vitejs/plugin-react": "^4.0.3",
		"@vitest/coverage-v8": "^0.33.0",
		"autoprefixer": "^10.4.20",
		"cspell": "^8.14.4",
		"eslint": "^8.45.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-consistently-named-imports": "^0.1.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.47",
		"prettier": "^2.8.8",
		"prettier-plugin-organize-imports": "^3.2.3",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"pretty-quick": "^3.1.3",
		"typescript": "^5.6.2",
		"vite": "^4.4.4",
		"vite-plugin-svgr": "^3.2.0",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.33.0"
	}
}
